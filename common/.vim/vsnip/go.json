{
  "iferr": {
    "prefix": "iferr",
    "description": "Expansion `if err != nil ...`",
    "body": [
      "if err != nil {",
      "\t$1",
      "}"
    ]
  },
  "ims": {
    "prefix": "ims",
    "body": [
      "import (",
      "\t$1",
      "}"
    ]
  },
  "struct": {
    "prefix": "STR",
    "description": "struct T { ... }",
    "body": [
      "type $1 struct {",
      "\t$2",
      "}"
    ]
  },
  "interface": {
    "prefix": "IF",
    "description": "interface I { ... }",
    "body": [
      "type $1 interface {",
      "\t$2",
      "}"
    ]
  },
  "error return": {
    "prefix": "rerr",
    "description": "if err != nil { return err }",
    "body": [
      "if err != nil {",
      "\treturn err",
      "}"
    ]
  },
  "error panic": {
    "prefix": "perr",
    "description": "if err != nil { return panic(err) }",
    "body": [
      "if err != nil {",
      "\tpanic(err)",
      "}"
    ]
  },
  "tdd func": {
    "prefix": "ftdd",
    "description": "generate table driven test",
    "body": [
      "func Test$1(t *testing.T) {",
      "\tt.Parallel()",
      "",
      "\tpatterns := []struct {",
      "\t\tname string",
      "\t}{",
      "\t\t{",
      "\t\t\tname: \"success\",",
      "\t\t},",
      "\t}",
      "",
      "\tfor _, tt := range patterns {",
      "\t\ttt := tt",
      "\t\tt.Run(tt.name, func(t *testing.T) {",
      "\t\t\tt.Parallel()",
      "",
      "\t\t\t$3",
      "\t\t})",
      "\t}",
      "}"
    ]
  }
}

